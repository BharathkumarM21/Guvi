print("-----Calculator-----")
m , n = input().split()
m=int(m)
n=int(n)
print("1.Subtract  2.Divide  3.Add  4.Multiply  5.Modulo-Divison" )
number = input("Enter Your Values: ")
if (number == "1"):
  answer = m - n
  print(answer)
if (number == "2"):
  answer = m / n
  print(answer)
if (number == "3"):
  answer = m + n
  print(answer)
if (number == "4"):
  answer=m * n
  print(answer)
if (number == "5"):
  answer = m % n
  print(answer)
else:
  print("****Re-Enter the value again****")
