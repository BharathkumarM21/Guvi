print("CALUCULATING TWO NUMBERS")
a=float(input())
b=float(input())
print("1.add \n 2.subtract \n 3.divide \n 4.multiply \n 5.modulo" )
choice=input("enter your choice")
if choice=="1":
  ans=a+b
  print(ans)
if choice=="2":
  ans=a-b
  print(ans)
if choice=="3":
  ans=a/b
  print(ans)
if choice=="4":
  ans=a*b
  print(ans)
if choice=="5":
  ans=a%b
  print(ans)
else:
  print("Please enter the choice given")
